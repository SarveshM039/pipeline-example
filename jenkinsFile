pipeline{
    agent any

     tools {
        maven 'Maven' 
    }

    environment{
        VERSION_NAME="1.34"
    }

    stages{
        stage("compile"){
            steps{
           sh 'javac Test.java'
           sh 'echo "${VERSION_NAME}"'
           sh 'echo "${maven}"'
           }
        }

           stage("run"){
            steps{
           sh 'java Test'
           }
        }
    }

    post{
        always{
            sh 'echo "always"'
        }

        success{
            sh 'echo "build Success"'
        }

        failure{
            sh 'echo "failure"'
        }
    }
}